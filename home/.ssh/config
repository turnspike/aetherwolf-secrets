Host github.com
    Hostname github.com
    User git
    IdentityFile ~/.ssh/id_rsa

Host gitlab.com
    Hostname gitlab.com
    User git
    IdentityFile ~/.ssh/id_rsa

Host bitbucket.org
    Hostname bitbucket.org
    User git
    IdentityFile ~/.ssh/id_rsa

Host hashbang
  HostName sf1.hashbang.sh
  IdentitiesOnly yes
  User yourusername
  IdentityFile ~/.ssh/id_rsa
